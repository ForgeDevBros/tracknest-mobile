{
  "expo": {
    "name": "tracknest",
    "slug": "tracknest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.jpeg",
    "splash": {
      "image": "./assets/images/splash-icon.jpeg",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "scheme": "com.forgedevbros.tracknest",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "googleClientId": "${process.env.EXPO_PUBLIC_GOOGLE_CLIENT_ID}"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}